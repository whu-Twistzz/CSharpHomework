	<script>
		function redirectToGetHelpApp() {
			var getHelpPartnerId = "cuv2ghredirect";
			var incrementedCv = '';

			if (window && window.awa && window.awa.cv && typeof (window.awa.cv.increment) == "function") {
				if (window.awa.cv.canIncrement()) {
					incrementedCv = window.awa.cv.increment();
				} else {
					incrementedCv = window.awa.cv.init();
				}
			}

			window.open(buildGetHelpAppProtocolLauncherUrl(incrementedCv, getHelpPartnerId), "_self");
		}

		function buildGetHelpAppProtocolLauncherUrl(cv, getHelpPartnerId) {
			var flightPreservationQueryStringKey = "flighting";
			var correlationVectorKey = "ms-cv";
			var getHelpAppProtocol = "ms-contact-support://";
			var assistedSupportIdKey = "ms-asid";
			var queryParamObj = {};
			var flights = "";

			queryParamObj[flightPreservationQueryStringKey] = flights;
			queryParamObj[correlationVectorKey] = cv;
			queryParamObj[assistedSupportIdKey] = "f133f9e0-439d-4e95-98e2-b68b99584b5a";

			return getHelpAppProtocol + getHelpPartnerId + createQueryStringFromObject(queryParamObj) || "";
		};

		function createQueryStringFromObject(paramsObject) {
			if (typeof (paramsObject) !== "object" || paramsObject === null) {
				return "";
			}

			var queryString = "";
			var isFirstParam = true;

			for (var key in paramsObject) {
				if (!paramsObject.hasOwnProperty(key)
					|| (typeof (paramsObject[key]) !== "string" && typeof (paramsObject[key]) !== "boolean" && typeof (paramsObject[key]) !== "number")
					|| paramsObject[key] === "") {
					continue;
				}

				if (isFirstParam) {
					queryString += "?";
					isFirstParam = false;
				} else {
					queryString += "&";
				}

				queryString += key + "=" + encodeURIComponent(paramsObject[key].toString());
			}

			return queryString;
		};

		function startWebExp() {
			if ("True" === "True") {
				return expWindow = window.open('https://support.microsoft.com/en-us/home/expcontact?AppVer=smcexp1' + '&' + 'ContactUsExperienceEntryPointAssetId=S.HP.SmcContactUsLanding', '_self');
			}
			else
			{
				var flowId = "smc-contactus-entitlement";
				var isSessionAccessible = window && window.sessionStorage || false;
				if (isSessionAccessible && !!window.sessionStorage.getItem("va-entry-clicked"))
				{
					flowId = "smc-va-to-contactus";
				}
				var partnerId = "smc";
				var config = { flowId: flowId, partnerId: partnerId };
				config.referrer = getReferrerDomain();

				var queryString = Object.keys(config).map(function(param) {
					return config[param] ? param + '=' + config[param] : '';
				}).filter(function(query) {
					return !!query;
				}).join('&');
				var url = "/virtual-agent/?" + queryString;

				var openContactWindow = function(link) {
					var url = createContactLink(link);
					var options = {
						width: 560,
						height: 630,
						left: 120,
						top: 40,
						resizable: 'yes',
						scrollbars: 'yes'
					};

					var featureString = Object.keys(options).map(function(feature) {
						return feature + '=' + options[feature];
					}).join(',');

					// Need to fix dual screen position
					var newWindow = window.open(url, "contactSupport", featureString);

					// Puts focus on the newWindow
					if (newWindow.focus)
					{
						newWindow.focus();
					}

					return newWindow;
				};

				var createContactLink = function (link) {
					return appendCurrentRouteParams('https://' +
						window.location.host +
						'/' +
						'en-us' +
						'/contact' +
						link);
				}

				openContactWindow(url);
			}
		}

		function getReferrerDomain() {
			var urlRegex = /\/\/(.*?)(\/|$|\?)/;
			if (window.document.referrer && urlRegex.test(window.document.referrer)) {
				return window.document.referrer.match(urlRegex)[1];
			} else {
				return null;
			}
		};

		// this will take query params in the contact us page to the new page url, may be removed.
		function appendCurrentRouteParams(url) {
			var parseQueryString = function () {
				var str = window.location.search;
				var objURL = {};
				str.replace(
					new RegExp("([^?=&]+)(=([^&]*))?", "g"),
					function ($0, $1, $2, $3) {
						objURL[$1] = $3;
					}
				);
				return objURL;
			};
			var params = parseQueryString();
			var queryIndex;
			if (Object.keys(params).length > 0 && url) {
				// Check if url already has query string
				if (queryIndex = url.indexOf('?') < 0) {
					url += '?';
				} else if (queryIndex < url.length - 1) {
					url += '&';
				}
				for (query in params) {
					var newQuery = query + '=';
					//Append the new param/query if not already exists
					if (url.toLowerCase().indexOf(newQuery.toLowerCase()) < 0)// do a case insensitive comparison
						url += newQuery + params[query] + '&';
				}
				url = url.slice(0, -1); // remove ending &
			}
			return url;
		};

	</script>


	

