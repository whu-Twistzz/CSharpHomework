(function () {
	var config = {
		syncMuid: typeof window.siteConsent !== "undefined" ? window.siteConsent.getConsentFor(WcpConsent.consentCategories.Analytics) : typeof window.mscc !== "undefined" ? window.mscc.hasConsent() : false,
		autoCapture: {
			assets: false,
			scroll: false,
			resize: true,
			addin: navigator.userAgent.search("MSAppHost") === -1	// If we are running in a WinRT host, disable collection of addins - causes APPHOST9603 error
		},
		coreData: {
			appId: 'support.office.com',
			pageName: "",
			pageTags: {}
		},
		isLoggedIn: false,
		authMethod: Number(0),
		callback: {
			pageViewPageTags: returnPageInfo,
			pageActionPageTags: returnPageInfo,
			contentUpdatePageTags: returnPageInfo,
			userConsentDetailsCallback: function () {
				return window.siteConsent ? window.siteConsent.getConsent() : null;
			}
		}
	};

	if (document.getElementsByClassName('ocpEditArticleLink').length > 0) {
		config.coreData.pageTags['ver'] = 'editShown';
	}


	awa.init(config);

	function returnPageInfo() {
		return config.coreData.pageTags;
	};
})();
</script>
