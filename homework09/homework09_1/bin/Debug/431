    <script>
        /**/
        $(function () {
            $('.dropdown-btn, .dl-platforms-popup button').click(function () {
                $('.dl-platforms-popup').toggleClass('dn');
            });
            $(".dropbtn").click(function () {
                var btnattr = $(".c-combo").attr("aria-expanded");
                if (btnattr == "false") {
                    $(".c-combo").attr("aria-expanded", "true");
                }
                else {
                    $(".c-combo").attr("aria-expanded", "false");
                }
            });
            $(".m-hero-item .c-combo .popup-btn, .m-hero-item .c-combo ul li[data-js-dialog-show='flow-dialog-scroll']").click(function () {
                $(".c-combo").attr("aria-expanded", "false");
                $("#mainContent").addClass("bgblur");
            });
            $(".c-dialog.f-flow button.glyph-cancel, .c-dialog .popup-overlay").click(function () {
                $("#mainContent").removeClass("bgblur");
            });
        });
    </script>

