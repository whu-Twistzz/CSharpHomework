<script>
    /*sticky nav*/
    $(document).ready(function () {
        $(window).scroll(function (event) {
            var scroll = $(window).scrollTop();
            var scrollTopPosition = $('#stickynavbanner,#banner,.for-sticky-banner,.m-feature,.m-content-placement').offset().top - 50;
            //console.log(scroll, scrollTopPosition);
            if (scroll >= scrollTopPosition) {
                //console.log('sticky added');
                $('#sticky-navbar').addClass('sticky');
                $('#sticky-navbar').removeClass('hide');
            } else {
                //console.log('sticky removced');
                $('#sticky-navbar').removeClass('sticky');
                $('#sticky-navbar').addClass('hide');
            }
        });
        $(".mobile-links div").each(function (e) {
            if (e != 0)
                $(this).hide();
        });
        $("#next").click(function () {
            if ($(".mobile-links div:visible").next().length != 0)
                $(".mobile-links div:visible").next().show().prev().hide();
            else {
                $(".mobile-links div:visible").hide();
                $(".mobile-links div:first").show();
            }
            return false;
        });
        $("#prev").click(function () {
            if ($(".mobile-links div:visible").prev().length != 0)
                $(".mobile-links div:visible").prev().show().next().hide();
            else {
                $(".mobile-links div:visible").hide();
                $(".mobile-links div:last").show();
            }
            return false;
        });
    });
</script>
</div> <div class="" data-grid="container">
    
