    <script>
        mwf.ComponentFactory.create([
            { 'component': mwf.SingleSlideCarousel },
            { 'component': mwf.Pivot },
            { 'component': mwf.Drawer },
            { 'component': mwf.RefineMenu },
            { 'component': mwf.ContentToggle },
            { 'component': mwf.Select },
            { 'component': mwf.MultiFeature },
            { 'component': mwf.Dialog },
            { 'component': mwf.Tooltip },
            { 'component': mwf.MultiSlideCarousel }
        ]);

        window.onload = function() {
            Cortex.Generic.calculateComponentLength();
            Cortex.Generic.setLinkTargets();
            Cortex.Generic.stripEmptyAttrs();
            Cortex.Generic.fixFeatureBleed();
            Cortex.Generic.multiFeatureHandler();
            Cortex.Generic.navContentPlacementAccessibilityHandler();
            Cortex.Components.OfficeSignup.Init("C1_officeSignUp_id");
            Cortex.Components.HowToBuyModule.init();
            Cortex.Generic.MultiSlideCarousel();
            Cortex.Generic.tabbedFeatureHandler();
            //Cortex.Generic.featureSwap();
            Cortex.Generic.bettCampaignPopup();
            Cortex.Components.TeamsDemoParser.Init();
        };

        $(window).resize(function() {
            Cortex.Generic.fixFeatureBleed();
        });

        function launchImmersiveReader() {
            var text = document.getElementById('txtImmersiveReader').value;
            var content = {
                title: "Immersive Reader",
                chunks: [
                    {
                        content: text
                    }
                ]
            };
            var token = "0";
            //If token is an empty field, fallback to One Note Learning Tools page
            if (token !== "") {
                ImmersiveReader.launchAsync(token, content, { uiZIndex: 1000000 });
            } else if (token == "noToken") {
                window.open('https://www.onenote.com/learningtools', '_blank');
            }
        }

    </script>

