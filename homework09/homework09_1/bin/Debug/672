<script>
	$(".ctex-dropdown-button").click(function () {	
		let dropDownItems = $(this).parent().find(".ctex-dropdown-list");
		if (dropDownItems.hasClass("active")) {
			dropDownItems.removeClass("active");
			$(this).attr("aria-expanded", "false");
		}
		else {
			dropDownItems.addClass("active");
			dropDownItems.css("min-width", $(this).outerWidth());
			$(this).attr("aria-expanded", "true");
		}
	});
	
	const $menu = $('.dropdown');

	$(document).mouseup(e => {
		if (!$(".ctex-dropdown-button").is(e.target)
   		&& $(".ctex-dropdown-button").has(e.target).length === 0)
   		{
			$(".ctex-dropdown-list").removeClass("active");
			$(".ctex-dropdown-button").attr("aria-expanded", "false");
   		}
	});
</script><div data-grid="container">
